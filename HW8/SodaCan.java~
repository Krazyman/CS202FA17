public class SodaCan {
  private double h;
  private double r;
  
  public SodaCan(double height, double radius) {
    h = height;
    r = radius;
  }
  
  public double getSurfaceArea() {
    return (2 * Math.PI * r * h) + (2 * Math.PI *r*r);
  }
  
  public double getVolume() {
   return (Math.PI * r * r * h); 
  }
}
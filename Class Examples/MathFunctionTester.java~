/*****************************************
	* To test some mathematical functions
	* Name: Manwai Nguyen
	* Date: 8/30/2017
	***************************************/

import java.util.Scanner

public class MathFunctionTester {
	public static void main(String[] args){
		double x, y=4.0;
		System.out.println(Math.sqrt(y)); 
		int a=3, b=2;
		System.out.println(Math.pow(a, b));
		System.out.println(Math.sin(Math.PI/4.0));
		System.out.println(Math.cos(Math.PI/2.0));
		System.out.println(Math.tan(Math.PI/4.0));
		System.out.println(Math.toRadians(90));
		System.out.println(Math.toDegrees(Math.PI));
		System.out.println(Math.exp(2));
		System.out.println(360/6.2832);
		System.out.println(Math.log(7.389));
	}
}
